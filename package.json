{
  "name": "timer",
  "version": "1.0.0",
  "main": "src/index.ts",
  "repository": "git@github.com:qwp0905/lru-cache.git",
  "author": "qwp0905 <qwp0905@gmail.com>",
  "license": "Apache 2.0",
  "packageManager": "yarn@4.9.2",
  "scripts": {
    "build": "tsc -b tsconfig.build.json",
    "test": "jest",
    "test:cov": "jest --coverage",
    "bench": "node -r ts-node/register --expose-gc --max-old-space-size=8092 bench/index.ts"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "^22.13.1",
    "@typescript-eslint/eslint-plugin": "^8.21.0",
    "@typescript-eslint/parser": "^8.21.0",
    "eslint": "8.57.0",
    "jest": "^29.7.0",
    "jest-util": "^30.0.5",
    "prettier": "^3.4.2",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "roots": [
      "<rootDir>/test"
    ],
    "testRegex": "^.*\\.spec\\.ts$",
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "coverageDirectory": "coverage",
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": 80
      }
    },
    "testEnvironment": "node",
    "verbose": true
  }
}
